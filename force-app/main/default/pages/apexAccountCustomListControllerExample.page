<apex:page controller="AccountListController">
    <apex:pageBlock >
        <apex:pageBlockTable value="{!accounts}" var="acc">
            <apex:column value="{!acc.Name}"/>
            <apex:column value="{!acc.accountNumber}"/>
            <apex:column value="{!acc.industry}"/>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>